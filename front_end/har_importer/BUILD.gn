import("../../scripts/build/ninja/devtools_entrypoint.gni")
import("../../scripts/build/ninja/devtools_module.gni")

devtools_module("har_importer") {
  sources = [
    "HARFormat.js",
    "HARImporter.js",
  ]

  deps = [
    "../common:bundle",
    "../sdk:bundle",
  ]
}

devtools_entrypoint("bundle") {
  entrypoint = "har_importer.js"

  deps = [ ":har_importer" ]
}
