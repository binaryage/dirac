import("../../scripts/build/ninja/devtools_entrypoint.gni")
import("../../scripts/build/ninja/devtools_module.gni")

devtools_module("browser_sdk") {
  sources = [
    "IssuesManager.js",
    "LogManager.js",
    "RelatedIssue.js",
  ]

  deps = [
    "../common:bundle",
    "../sdk:bundle",
  ]
}

devtools_entrypoint("bundle") {
  entrypoint = "browser_sdk.js"

  deps = [ ":browser_sdk" ]
}
