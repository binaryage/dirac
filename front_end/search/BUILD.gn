import("../../scripts/build/ninja/devtools_entrypoint.gni")
import("../../scripts/build/ninja/devtools_module.gni")

devtools_module("search") {
  sources = [
    "SearchConfig.js",
    "SearchResultsPane.js",
    "SearchView.js",
  ]

  deps = [
    "../common:bundle",
    "../components:bundle",
    "../text_utils:bundle",
    "../ui:bundle",
    "../workspace:bundle",
