import("../../scripts/build/ninja/devtools_entrypoint.gni")
import("../../scripts/build/ninja/devtools_module.gni")

devtools_module("platform") {
  sources = [
    "UIString.js",
    "array-utilities.js",
    "date-utilities.js",
    "number-utilities.js",
    "set-utilities.js",
    "string-utilities.js",
    "utilities.js",
  ]

  deps = []
}

devtools_entrypoint("bundle") {
  entrypoint = "platform.js"

  deps = [ ":platform" ]
}
