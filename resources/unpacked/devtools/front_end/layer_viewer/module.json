{
  "dependencies": [
    "components",
    "sdk",
    "timeline_model",
    "ui",
    "perf_ui"
  ],
  "extensions": [
    {
      "type": "action",
      "actionId": "layers.reset-view",
      "category": "Layers",
      "title": "Reset view",
      "bindings": [
        {
          "shortcut": "0"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.pan-mode",
      "category": "Layers",
      "title": "Switch to pan mode",
      "bindings": [
        {
          "shortcut": "x"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.rotate-mode",
      "category": "Layers",
      "title": "Switch to rotate mode",
      "bindings": [
        {
          "shortcut": "v"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.zoom-in",
      "category": "Layers",
      "title": "Zoom in",
      "bindings": [
        {
          "shortcut": "Shift+Plus"
        },
        {
          "shortcut": "NumpadPlus"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.zoom-out",
      "category": "Layers",
      "title": "Zoom out",
      "bindings": [
        {
          "shortcut": "Shift+Minus"
        },
        {
          "shortcut": "NumpadMinus"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.up",
      "category": "Layers",
      "title": "Pan or rotate up",
      "bindings": [
        {
          "shortcut": "Up"
        },
        {
          "shortcut": "w"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.down",
      "category": "Layers",
      "title": "Pan or rotate down",
      "bindings": [
        {
          "shortcut": "Down"
        },
        {
          "shortcut": "s"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.left",
      "category": "Layers",
      "title": "Pan or rotate left",
      "bindings": [
        {
          "shortcut": "Left"
        },
        {
          "shortcut": "a"
        }
      ]
    },
    {
      "type": "action",
      "actionId": "layers.right",
      "category": "Layers",
      "title": "Pan or rotate right",
      "bindings": [
        {
          "shortcut": "Right"
        },
        {
          "shortcut": "d"
        }
      ]
    }
  ],
  "modules": [
    "layer_viewer.js",
    "layer_viewer-legacy.js",
    "LayerDetailsView.js",
    "LayerTreeOutline.js",
    "LayerViewHost.js",
    "Layers3DView.js",
    "PaintProfilerView.js",
    "TransformController.js"
  ],
  "resources": [
    "layers3DView.css",
    "paintProfiler.css",
    "layerDetailsView.css"
  ]
}